{
  "compilerOptions": {
    "allowJs": false,
    "baseUrl": "./src",
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "lib": ["esnext", "dom"],
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "paths": {
      "@/*": ["./*"]
    },
    "strict": true,
    "target": "esnext"
  },
  "include": ["./src/"]
}
